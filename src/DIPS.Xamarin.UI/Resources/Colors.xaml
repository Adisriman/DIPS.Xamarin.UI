    <Color x:Key="StatusColorPalette.InfoDark">#266B89</Color>
    <Color x:Key="StatusColorPalette.Info">#337AB7</Color>
    <Color x:Key="StatusColorPalette.InfoLight">#7FAFD8</Color>
