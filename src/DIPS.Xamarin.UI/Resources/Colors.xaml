    <Color x:Key="Theme.Teal.Primary">#047F89</Color>
    <Color x:Key="Theme.Teal.PrimaryLight">#65868F</Color>
    <Color x:Key="Theme.Teal.PrimaryAir">#98B2AE</Color>
    <Color x:Key="Theme.Teal.Secondary">#97C8CD</Color>
    <Color x:Key="Theme.Teal.SecondaryLight">#ECF3F4</Color>
    <Color x:Key="Theme.Teal.SecondaryAir">#F0F5F7</Color>


    <!-- Color Palette  -->
    <!-- Dark -->
    <Color x:Key="ColorPalette.Dark">#000000</Color>
    <Color x:Key="ColorPalette.DarkLight">#111111</Color>
    <Color x:Key="ColorPalette.DarkAir">#3B3D3D</Color>
    <!-- Tertiary -->
    <Color x:Key="ColorPalette.Tertiary">#404040</Color>
    <Color x:Key="ColorPalette.TertiaryLight">#4A4A4A</Color>
